{
	"name": "eleventy-plugin-icons",
	"version": "4.5.7",
	"description": "A plugin for Eleventy to add and transform icons from any icon set.",
	"author": "uncenter <hi@uncenter.dev>",
	"repository": "uncenter/eleventy-plugin-icons",
	"license": "MIT",
	"files": [
		"LICENSE",
		"README.md",
		"dist/"
	],
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"type": "module",
	"scripts": {
		"build": "unbuild",
		"test": "vitest",
		"bench": "vitest bench",
		"lint": "biome lint .",
		"format": "biome check --write ."
	},
	"keywords": [
		"11ty",
		"eleventy",
		"eleventy-plugin"
	],
	"dependencies": {
		"fast-xml-parser": "^4.4.0",
		"just-extend": "^6.2.0",
		"just-memoize": "^2.2.0",
		"just-typeof": "^3.2.0",
		"kleur": "^4.1.5"
	},
	"devDependencies": {
		"@11ty/eleventy": "3.0.0",
		"@biomejs/biome": "1.9.4",
		"@codspeed/vitest-plugin": "^5.0.1",
		"@types/node": "^22.13.11",
		"lucide-static": "^0.483.0",
		"typescript": "^5.8.2",
		"unbuild": "^3.5.0",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=16.20.2"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"esbuild"
		]
	}
}
