{
	"name": "eleventy-plugin-icons",
	"version": "4.3.0",
	"description": "A plugin for Eleventy to add and transform icons from any icon set.",
	"author": "uncenter <hi@uncenter.dev>",
	"repository": {
		"type": "git",
		"url": "https://github.com/uncenter/eleventy-plugin-icons.git"
	},
	"scripts": {
		"build": "unbuild",
		"test:install": "cd test/live && pnpm install",
		"test": "pnpm test:install && vitest",
		"format": "prettier --write ."
	},
	"keywords": [
		"11ty",
		"eleventy",
		"eleventy-plugin"
	],
	"license": "MIT",
	"files": [
		"LICENSE",
		"README.md",
		"dist/"
	],
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"type": "module",
	"dependencies": {
		"fast-xml-parser": "^4.2.7",
		"just-extend": "^6.2.0",
		"just-memoize": "^2.2.0",
		"kleur": "^4.1.5"
	},
	"devDependencies": {
		"@types/node": "^20.5.9",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^9.0.0",
		"prettier": "^3.0.3",
		"unbuild": "^2.0.0",
		"vitest": "^0.34.3"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"packageManager": "pnpm@8.7.6"
}
