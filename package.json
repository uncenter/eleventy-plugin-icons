{
	"name": "eleventy-plugin-icons",
	"version": "4.5.0",
	"description": "A plugin for Eleventy to add and transform icons from any icon set.",
	"author": "uncenter <hi@uncenter.dev>",
	"repository": "uncenter/eleventy-plugin-icons",
	"license": "MIT",
	"files": [
		"LICENSE",
		"README.md",
		"dist/"
	],
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"type": "module",
	"scripts": {
		"build": "unbuild",
		"test:setup": "pnpm build && cd test/fixtures && pnpm eleventy",
		"test": "pnpm run test:setup && vitest",
		"lint": "eslint .",
		"format": "prettier --write ."
	},
	"keywords": [
		"11ty",
		"eleventy",
		"eleventy-plugin"
	],
	"dependencies": {
		"fast-xml-parser": "^4.3.6",
		"just-extend": "^6.2.0",
		"just-memoize": "^2.2.0",
		"just-typeof": "^3.2.0",
		"kleur": "^4.1.5"
	},
	"devDependencies": {
		"@11ty/eleventy": "3.0.0-alpha.10",
		"@types/node": "^20.12.2",
		"@typescript-eslint/eslint-plugin": "^7.4.0",
		"@typescript-eslint/parser": "^7.4.0",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-unicorn": "^51.0.1",
		"eslint-plugin-vitest": "^0.4.1",
		"lucide-static": "^0.379.0",
		"prettier": "^3.2.4",
		"typescript": "^5.4.3",
		"unbuild": "^2.0.0",
		"vitest": "^1.4.0"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"packageManager": "pnpm@9.1.3"
}
