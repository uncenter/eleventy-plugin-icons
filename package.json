{
	"name": "eleventy-plugin-icons",
	"version": "4.4.0",
	"description": "A plugin for Eleventy to add and transform icons from any icon set.",
	"author": "uncenter <hi@uncenter.dev>",
	"repository": {
		"type": "git",
		"url": "https://github.com/uncenter/eleventy-plugin-icons.git"
	},
	"license": "MIT",
	"files": [
		"LICENSE",
		"README.md",
		"dist/"
	],
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"type": "module",
	"scripts": {
		"build": "unbuild",
		"test:install": "cd test/live && pnpm install",
		"test": "pnpm test:install && vitest",
		"lint": "eslint .",
		"format": "prettier --write ."
	},
	"keywords": [
		"11ty",
		"eleventy",
		"eleventy-plugin"
	],
	"dependencies": {
		"fast-xml-parser": "^4.3.2",
		"just-extend": "^6.2.0",
		"just-memoize": "^2.2.0",
		"just-typeof": "^3.2.0",
		"kleur": "^4.1.5"
	},
	"devDependencies": {
		"@types/node": "^20.8.4",
		"@typescript-eslint/eslint-plugin": "^6.7.5",
		"@typescript-eslint/parser": "^6.7.5",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-unicorn": "^49.0.0",
		"eslint-plugin-vitest": "^0.3.2",
		"prettier": "^3.0.3",
		"typescript": "^5.2.2",
		"unbuild": "^2.0.0",
		"vitest": "^0.34.6"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"packageManager": "pnpm@8.9.0"
}
